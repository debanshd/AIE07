{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/simple_api.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/simple_api.py"
    },
    {
      "src": "/",
      "dest": "/frontend/backend_demo.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "api",
    "VERCEL_ENV": "preview"
  }
} 